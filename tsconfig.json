{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "moduleResolution": "node",
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "src",
    "skipLibCheck": true,
    "allowJs": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "rootDir": "./",
    "paths": {
      "modules/*": ["./modules/*"],
      "models/*": ["./models/*"],
      "common/*": ["./common/*"],
      "providers/*": ["./providers/*"]
    }
  },
}
